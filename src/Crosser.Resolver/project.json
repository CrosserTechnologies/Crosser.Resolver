    "net451": {
      "buildOptions": {
        "define": [ "NET" ]
      }
    },
    "net452": {
      "buildOptions": {
        "define": [ "NET" ]
      }
    },
    "net46": {
      "buildOptions": {
        "define": [ "NET" ]
      }
